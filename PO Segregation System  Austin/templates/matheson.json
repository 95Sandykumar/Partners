      "example": "4647948"
    },
    "po_date": {
      "label": "DATE:",
      "pattern": "DATE:\\s*(\\d{2}/\\d{2}/\\d{2})",
      "format": "MM/DD/YY"
    },
    "vendor_num": {
      "label": "VENDOR NUM:",
      "pattern": "VENDOR NUM:\\s*([A-Z]\\d+)"
