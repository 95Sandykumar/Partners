      "column_headers": ["LINE NUM", "QUANTITY", "UOM", "SUP", "ITEM", "ITEM DESCRIPTION", "UNIT PRICE", "EXTENDED PRICE", "DUE DATE"],
      "part_number_column": "ITEM",
      "supplier_code_column": "SUP",
