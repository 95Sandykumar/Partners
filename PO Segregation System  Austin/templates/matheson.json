    "vendor_info": {
      "location": "left_box",
      "label": "VENDOR",
      "box_style": "bordered"
    },
    "ship_to": {
      "location": "right_box",
      "label": "SHIP TO",
      "box_style": "bordered"
    },
    "invoice_to": {
      "location": "top_right_box",
      "label": "INVOICE"
    },
    "line_items": {
      "table_structure": "formal_table_with_colored_headers",
