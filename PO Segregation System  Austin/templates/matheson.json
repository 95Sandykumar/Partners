    "special_instructions": {
      "location": "above_table",
      "pattern": "\\*\\*.*?\\*\\*"
    },
    "totals": {
      "label": "TOTAL",
      "location": "bottom_right"
    }
  },
  "part_number_patterns": [
    "^CMD\\s*\\d+$",
    "^\\d{3}-\\d{2}-\\d{3}$"
  ],
  "field_mappings": {
    "their_part_to_extract": "ITEM",
