    },
    "BER-": {
      "manufacturer": "Bernard",
      "notes": "Bernard welding products"
    },
    "LIN-": {
      "manufacturer": "Lincoln",
      "notes": "Lincoln Electric products"
    }
  },
  "field_mappings": {
    "their_part_to_extract": "Item",
