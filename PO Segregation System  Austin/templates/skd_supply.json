  "extraction_rules": {
    "po_number": {
      "location": "top_right",
      "label": "P.O. No.",
      "pattern": "P\\.O\\.\\s*No\\.\\s*(\\d+)",
      "example": "2183"
    },
    "po_date": {
      "label": "Date",
      "pattern": "Date\\s*(\\d{1,2}/\\d{1,2}/\\d{4})",
      "format": "M/D/YYYY"
    },
    "vendor_info": {
      "location": "left_box",
      "label": "Vendor"
    },
    "ship_to": {
      "location": "right_box",
      "label": "Ship To"
    },
    "terms": {
      "columns": ["Terms", "Due Date", "Ship Via"],
      "location": "below_addresses"
    },
    "line_items": {
      "table_structure": "clean_table",
      "column_headers": ["Item", "Description", "Qty", "", "Rate", "Amount"],
      "part_number_column": "Item",
      "uom_in_qty_column": true,
      "notes": "Item column contains full part codes with prefixes indicating manufacturer"
    },
    "totals": {
      "label": "Total",
      "location": "bottom_right"
    }
  },
  "part_number_patterns": [
    "^CMI-[A-Z]\\d+$",
    "^BER-[A-Z0-9]+$",
    "^LIN-[A-Z0-9-]+$"
  ],
  "part_number_prefixes": {
    "CMI-": {
      "manufacturer": "CM Industries",
